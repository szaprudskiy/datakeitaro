{
  "version": 2,
  "builds": [
    {
      "src": "frontend/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "frontend/js/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/js/(.*)",
      "dest": "frontend/js/$1"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/index.html"
    },
    {
      "src": "/getdata",
      "dest": "app.js"
    }
  ]
}
